<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent"
    android:id="@+id/linearlayout_0"
    android:layout_width="match_parent">
    <Button
        android:id="@+id/scan_code_btn"
        android:layout_height="50dp"
        android:layout_width="match_parent"
        android:text="scan code"
        />


<android.support.v7.widget.RecyclerView

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_below="@+id/scan_code_btn"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".TransGetFragment"
    tools:showIn="@layout/get_trans_list"
    android:id="@+id/recyclerView_Trans_list"
    app:layoutManager="android.support.v7.widget.LinearLayoutManager"
    tools:listitem="@layout/get_trans_list" >
</android.support.v7.widget.RecyclerView>
</RelativeLayout>


